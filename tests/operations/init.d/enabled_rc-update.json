{
    "args": ["nginx"],
    "kwargs": {
        "enabled": true
    },
    "facts": {
        "sysvinit.InitdStatus": {
            "nginx": true
        },
        "find_links": {
            "/etc/rc*.d/S*nginx": []
        },
        "linux_distribution": {
            "name": "Gentoo"
        }
    },
    "commands": [
        "rc-update add nginx default"
    ],
    "idempotent": false
}
